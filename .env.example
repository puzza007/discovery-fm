# Last.fm API credentials
# Get yours at: https://www.last.fm/api/account/create
LASTFM_API_KEY=your_lastfm_api_key_here
LASTFM_API_SECRET=your_lastfm_api_secret_here

# Spotify API credentials
# Get yours at: https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Spotify OAuth redirect URI (must match your Spotify app settings)
SPOTIFY_REDIRECT_URI=http://localhost:8888/callback
